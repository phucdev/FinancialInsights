TZ=Europe/Berlin

# MySQL
MYSQL_ROOT_PASSWORD=changeme
MYSQL_DATABASE=market
MYSQL_USER=app
MYSQL_PASSWORD=app_pw

# API
API_IMAGE=finproto/api:dev
API_PORT=8000

# Kafka (KRaft)
# Use this from containers (internal DNS)
KAFKA_BROKER=broker:29092
# Use this from your host (producers/consumers you run with uv)
KAFKA_BROKER_HOST=localhost:9092
# topics: name:partitions:replication
KAFKA_TOPICS=news.raw:1:1,prices.raw:1:1

# Alpha Vantage API for time series stock data and financial news
ALPHAVANTAGE_API_KEY=your_alpha_vantage_api_key_here
